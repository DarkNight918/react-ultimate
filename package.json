{
  "name": "react-starter",
  "version": "0.1.0",
  "main": "backend/scripts/app.js",
  "scripts": {
    "lint": "eslint .",
    "build": "webpack --config webpack.config-prod.js --progress --profile --colors",
    "dev": "webpack-dev-server --config webpack.config-dev.js --progress --colors --port 2992 --inline",
    "node": "NODE_ENV=production babel-node backend/scripts/app.js",
    "nodemon": "nodemon backend/scripts/app.js",
    "release-patch": "git checkout master && npm version patch && git push origin master --tags",
    "release-minor": "git checkout master && npm version minor && git push origin master --tags",
    "release-major": "git checkout master && npm version major && git push origin master --tags",
    "release-as-lib": "git checkout master && npm run prod && git commit -am 'Distribute' && npm version major && git push origin master --tags",
    "test": "mocha --opts specs/mocha.opts specs",
    "test-watch": "mocha --opts specs/mocha.opts -w specs"
  },
  "dependencies": {
    "axios": "^0.5.4",
    "babel": "^5.4.7",
    "babel-core": "^5.4.7",
    "babel-loader": "^5.1.3",
    "babelify": "^6.1.0",
    "baobab": "^1.1.0",
    "baobab-react": "^0.1.1",
    "body-parser": "^1.12.4",
    "bower": "^1.4.1",
    "chai": "^2.3.0",
    "classnames": "^2.1.1",
    "compression": "^1.4.4",
    "config": "^1.13.0",
    "cookie-parser": "^1.3.4",
    "deep-merge": "^1.0.0",
    "emailjs": "^0.3.16",
    "event-stream": "^3.3.1",
    "express": "^4.12.3",
    "faker": "^2.1.3",
    "flat": "^1.5.1",
    "glob": "^5.0.9",
    "gulp": "^3.8.11",
    "gulp-cached": "^1.1.0",
    "gulp-less": "^3.0.3",
    "gulp-plumber": "^1.0.0",
    "helmet": "^0.9.0",
    "joi": "^6.4.1",
    "lodash.debounce": "^3.0.3",
    "lodash.result": "^3.1.1",
    "lodash.throttle": "^3.0.2",
    "marked": "^0.3.3",
    "mkdirp": "^0.5.0",
    "moment": "^2.10.3",
    "morgan": "^1.5.3",
    "node-uuid": "^1.4.3",
    "nunjucks": "^1.3.4",
    "nunjucks-markdown": "^1.0.0",
    "paqmind.data-lens": "^0.3.0",
    "ramda": "^0.14.0",
    "react": "^0.13.3",
    "react-document-title": "^1.0.1",
    "react-hot-loader": "^1.2.7",
    "react-proxy-loader": "^0.3.4",
    "react-router": "^0.13.2",
    "readable-stream": "^1.1.13",
    "require-directory": "^2.1.0",
    "run-sequence": "^1.1.0",
    "util-inspect": "^0.1.8",
    "webpack": "^1.9.9",
    "webpack-dev-server": "^1.9.0",
    "winston": "^1.0.0",
    "winston-mail": "^0.4.0",
    "yargs": "^3.9.1"
  },
  "devDependencies": {
    "babel-eslint": "^3.1.9",
    "mocha": "^2.2.5",
    "mocha-clean": "^0.4.0",
    "nodemon": "^1.3.7",
    "eslint": "^0.21.0"
  }
}
